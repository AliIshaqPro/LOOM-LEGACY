      <script>
         // Retract the navbar when a nav item is clicked
         document.querySelectorAll('.nav-link').forEach(function(link) {
             link.addEventListener('click', function() {
                 var navbarCollapse = document.querySelector('.navbar-collapse');
                 var bootstrapCollapse = new bootstrap.Collapse(navbarCollapse);
                 bootstrapCollapse.hide();
             });
         });
         
         document.addEventListener("DOMContentLoaded", function() {
         const kpis = document.querySelectorAll('.kpi');
         
         const observer = new IntersectionObserver((entries) => {
           entries.forEach(entry => {
             if (entry.isIntersecting) {
               entry.target.classList.add('visible'); // Add 'visible' class when in viewport
             } else {
               entry.target.classList.remove('visible'); // Remove 'visible' class when out of viewport
             }
           });
         }, { threshold: 0.1 }); // Trigger when 10% of the element is visible
         
         kpis.forEach(kpi => {
           observer.observe(kpi); // Observe each .kpi element
         });
         });
         document.getElementById('toggleCardButton').addEventListener('click', function() {
    var card = document.getElementById('feedbackCard');
    if (card.style.display === 'none' || card.style.display === '') {
      card.style.display = 'block';
      this.textContent = 'Hide Feedback Card';
    } else {
      card.style.display = 'none';
      this.textContent = 'Please Give Your Valuable Feedback';
    }
  });

  document.getElementById('feedbackButton').addEventListener('click', function() {
    var message = prompt('Please enter your feedback:');
    if (message) {
      var form = document.createElement('form');
      form.method = 'POST';
      form.action = '/feedbacks';

      var messageInput = document.createElement('input');
      messageInput.type = 'hidden';
      messageInput.name = 'feedback[message]';
      messageInput.value = message;
      form.appendChild(messageInput);

      var authenticityToken = document.createElement('input');
      authenticityToken.type = 'hidden';
      authenticityToken.name = 'authenticity_token';
      authenticityToken.value = document.querySelector('meta[name="csrf-token"]').getAttribute('content');
      form.appendChild(authenticityToken);

      document.body.appendChild(form);
      form.submit();
    }
  });
      </script>
